<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="16.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox VBox.vgrow="NEVER" spacing="6.0">
         <children>
            <TextField fx:id="createField" HBox.hgrow="ALWAYS" />
            <Button fx:id="createButton" mnemonicParsing="false" text="add" HBox.hgrow="NEVER" />
         </children>
      </HBox>
      <ListView fx:id="itemList" VBox.vgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
   </padding>
</VBox>
